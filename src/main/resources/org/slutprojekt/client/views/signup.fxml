<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.slutprojekt.client.controllers.SignupController"
            prefHeight="400.0" prefWidth="600.0"
            alignment="CENTER" spacing="10.0">

    <TextField fx:id="usernameField" promptText="username"/>
    <PasswordField fx:id="passwordField1" promptText="password"/>
    <PasswordField fx:id="passwordField2" promptText="repeat password"/>
    <Button onAction="#submit" text="Signup"/>
    <Label fx:id="errorLabel"/>
</VBox>
